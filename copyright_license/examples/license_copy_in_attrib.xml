<knora xmlns="https://dasch.swiss/schema"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="https://dasch.swiss/schema ../../src/dsp_tools/resources/schema/data.xsd"
       shortcode="4124"
       default-ontology="testonto">

    <resource label="The only resource" restype=":minimalResource" id="the_only_resource">
        <bitstream
                license="Credit: Wellcome Collection."
                copyright="CC BY-NC 4.0"
        >testdata/bitstreams/test.tif</bitstream>
    </resource>

    <resource label="iiifLink" restype=":iiifLink" id="iiifLink_whitespaces">
        <iiif-uri
                license="Credit: Zebrafish embryo.
                Source: Wellcome Collection."
                copyright="CC BY-NC 4.0"
        >
        <!-- If any user would write a newline like this, lxml would parse it as many white-spaces but not as newline -->
            https://iiif.wellcomecollection.org/image/b20432033_B0008608.JP2/full/1338%2C/0/default.jpg</iiif-uri>
    </resource>


    <!-- To get a newline it must be encoded -->
    <resource label="iiifLink" restype=":iiifLink" id="iiifLink_encoded_newline">
        <iiif-uri
                license="Credit: Zebrafish embryo.&#10;Source: Wellcome Collection."
                copyright="CC BY-NC 4.0"
        >
            https://iiif.wellcomecollection.org/image/b20432033_B0008608.JP2/full/1338%2C/0/default.jpg</iiif-uri>
    </resource>



</knora>
