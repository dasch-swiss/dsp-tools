    "keywords": ["test", "knora-py"],
    "lists": [
      {
        "name": "options",
        "labels": {
          "en": "Options"
        },
        "nodes": [
          {
            "name": "opt1",
            "labels": {
              "en": "Opt 1"
            }
          },
          {
