<?xml version="1.0" encoding="UTF-8"?>
<knora xmlns="https://dasch.swiss/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://dasch.swiss/schema https://raw.githubusercontent.com/dasch-swiss/dsp-tools/main/src/dsp_tools/resources/schema/data.xsd" shortcode="00A2" default-ontology="dog">
    <permissions id="open">
        <allow group="UnknownUser">V</allow>
        <allow group="KnownUser">V</allow>
        <allow group="ProjectMember">D</allow>
        <allow group="ProjectAdmin">CR</allow>
    </permissions>

    <resource label="Fluffy Image" restype=":Image2D" id="Fluffy1" permissions="open">
        <bitstream permissions="open">images/Fluffy.jpg</bitstream>
        <text-prop name=":hasAttribution">
            <text permissions="open" encoding="utf8">Columbus Metropolitan Library, Public domain, via Wikimedia Commons</text>
        </text-prop>
        <uri-prop name=":hasSourceLink">
            <uri permissions="open">https://commons.wikimedia.org/wiki/File:Siberian_Husky_dog_-_DPLA_-_b729034a262a5abba8f565574bb84dae.jpg</uri>
        </uri-prop>
    </resource>

    <resource label="Fluffy Image" restype=":Image2D" id="Fluffy2" permissions="open">
        <bitstream permissions="open">images/subFolder/Fluffy.jpg</bitstream>
        <text-prop name=":hasAttribution">
            <text permissions="open" encoding="utf8">Columbus Metropolitan Library, Public domain, via Wikimedia Commons</text>
        </text-prop>
        <uri-prop name=":hasSourceLink">
            <uri permissions="open">https://commons.wikimedia.org/wiki/File:Siberian_Husky_dog_-_DPLA_-_5248117eea26fda68834d39ebc90f925.jpg</uri>
        </uri-prop>
    </resource>

    <resource label="Shiba Inu Image" restype=":Image2D" id="Shiba" permissions="open">
        <bitstream permissions="open">images/subFolder/ShibaInu.jpg</bitstream>
        <text-prop name=":hasAttribution">
            <text permissions="open" encoding="utf8">Picography, CC0, via Wikimedia Commons</text>
        </text-prop>
        <uri-prop name=":hasSourceLink">
            <uri permissions="open">https://commons.wikimedia.org/wiki/File:Picography-shiba-inu-dog-forest-walk-sm-1.jpg</uri>
        </uri-prop>
    </resource>

    <resource label="Good Girl Image" restype=":Image2D" id="GoodGirlImage" permissions="open">
        <bitstream permissions="open">images/GoodGirl.jpg</bitstream>
        <text-prop name=":hasAttribution">
            <text permissions="open" encoding="utf8">Oldsingerman20, CC0, via Wikimedia Commons</text>
        </text-prop>
        <uri-prop name=":hasSourceLink">
            <uri permissions="open">https://commons.wikimedia.org/wiki/File:New_Guinea_Singing_Dog_looking_up.jpg</uri>
        </uri-prop>
    </resource>

    <resource label="Fluffy" restype=":Dog" id="Fluffy" permissions="open">
        <resptr-prop name=":hasImage">
            <resptr permissions="open">Fluffy1</resptr>
            <resptr permissions="open">Fluffy2</resptr>
        </resptr-prop>
    </resource>
    
    <resource label="Shiba" restype=":Dog" id="ShibaDog" permissions="open">
        <resptr-prop name=":hasImage">
            <resptr permissions="open">Shiba</resptr>
        </resptr-prop>
    </resource>

    <resource label="Good Girl" restype=":Dog" id="GoodGirl" permissions="open">
        <resptr-prop name=":hasImage">
            <resptr permissions="open">GoodGirlImage</resptr>
        </resptr-prop>
    </resource>
</knora>
