<?xml version='1.0' encoding='utf-8'?>

<knora xmlns="https://dasch.swiss/schema"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="https://dasch.swiss/schema ../../../src/dsp_tools/resources/schema/data.xsd"
       shortcode="0011"
       default-ontology="onto">

    <!-- 4 Validation Errors -->

    <!-- The resource does not have a direct cardinality for hasText0 -->
    <resource label="ResourceSubCls1" restype=":ResourceSubCls1" id="ResourceSubCls1">
        <text-prop name=":hasText0">
            <text encoding="utf8">Text 0</text>
        </text-prop>
        <text-prop name=":hasTextSubProp1">
            <text encoding="utf8">Text 1</text>
        </text-prop>
        <text-prop name=":nonRelatedProp">
            <text encoding="utf8">Text non-related</text>
        </text-prop>
    </resource>

    <!-- The resource does not have a direct cardinality for hasTextSubProp1 and hasText0 -->
    <resource label="ResourceSubCls2" restype=":ResourceSubCls2" id="ResourceSubCls2">
        <text-prop name=":hasText0">
            <text encoding="utf8">Text 0</text>
        </text-prop>
        <text-prop name=":hasTextSubProp1">
            <text encoding="utf8">Text 1</text>
        </text-prop>
        <text-prop name=":hasTextSubProp2">
            <text encoding="utf8">Text 2</text>
        </text-prop>
        <text-prop name=":nonRelatedProp">
            <text encoding="utf8">Text non-related</text>
        </text-prop>
    </resource>

    <!-- The resource does not have a direct cardinality for hasText0 -->
    <resource label="ResourceUnrelated" restype=":ResourceUnrelated" id="ResourceUnrelated">
        <text-prop name=":hasText0">
            <text encoding="utf8">Text 0</text>
        </text-prop>
        <text-prop name=":hasTextSubProp1">
            <text encoding="utf8">hasTextSubProp1</text>
        </text-prop>
    </resource>

    <!--
        The resource is a sub-class and has no direct cardinality.
        Because the sub-class must also follow the super-class we get two violation reports for one error.
        This must be filtered out.
    -->
    <resource label="ResourceInexistentLinkDuplicateReport" restype=":SubLinkProp" id="ResourceInexistentLinkDuplicateReport">
        <resptr-prop name=":hasLink">
            <resptr>inexistent_id</resptr>
        </resptr-prop>
    </resource>

</knora>
