@prefix owl:         <http://www.w3.org/2002/07/owl#> .
@prefix rdf:         <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:        <http://www.w3.org/2000/01/rdf-schema#> .
@prefix salsah-gui:  <http://api.knora.org/ontology/salsah-gui/v2#> .
@prefix xsd:         <http://www.w3.org/2001/XMLSchema#> .
@prefix sh:          <http://www.w3.org/ns/shacl#> .

@prefix val-shapes:  <http://api.knora.org/val-shapes-shapes/v2#> .

@prefix onto:        <http://0.0.0.0:3333/ontology/9999/onto/v2#> .
@prefix onto-shapes: <http://0.0.0.0:3333/ontology/9999/onto-shapes/v2#> .
@prefix data:        <http://0.0.0.0:3333/ontology/9999/data#> .


data:cardViolation a onto:CardOneResource .


data:typeViolation a onto:CardOneResource ;
  onto:hasSimpleText data:bnText .

data:bnText a knora-api:TextValue ;
  knora-api:valueAsString "" .


data:ListViolation a onto:NormalClass ;
                   onto:listProp data:bnList .

data:bnList a knora-api:ListValue ;
    knora-api:listValueAsListNode "n1" ;
  val-shapes:hasListName "other" .

