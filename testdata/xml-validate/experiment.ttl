@prefix owl:         <http://www.w3.org/2002/07/owl#> .
@prefix rdf:         <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:        <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:         <http://www.w3.org/2001/XMLSchema#> .
@prefix sh:          <http://www.w3.org/ns/shacl#> .
@prefix dash:        <http://datashapes.org/dash#> .

@prefix val-shapes:  <http://api.knora.org/validation-shapes#> .
@prefix val-onto:    <http://api.knora.org/validation-onto#> .
@prefix ex:        <http://validation-9999/onto#> .
@prefix onto-shapes: <http://validation-9999/onto-shapes#> .

@prefix data: <http://validation-9999/data/> .

ex:age a owl:ObjectProperty  .
ex:otherNum a owl:ObjectProperty  .
ex:hasNumber a owl:DatatypeProperty .
ex:author a owl:ObjectProperty .
ex:Person a owl:Class .
ex:Provenance a owl:Class .
ex:Origin a owl:Class .

ex:Person_Shape a sh:NodeShape ;
                sh:targetClass ex:Person ;
                sh:property ex:Person_age_PropShape , ex:Person_otherNum_PropShape .


ex:Person_age_PropShape a sh:PropertyShape ;
                        sh:path ex:age ;
                        sh:node ex:age_NodeShape .

ex:age_NodeShape
  a sh:NodeShape ;
  sh:class ex:Provenance ;
  sh:property [
                a sh:PropertyShape ;
                sh:path ex:hasNumber ;
                sh:datatype xsd:integer ;
              ] ,
              [
                a sh:PropertyShape ;
                sh:path ex:author ;
                sh:class ex:Person ;
              ] .


ex:Person_otherNum_PropShape a sh:PropertyShape ;
                        sh:path ex:otherNum ;
                        sh:node ex:otherNum_NodeShape .

ex:otherNum_NodeShape
  a sh:NodeShape ;
  sh:class ex:Provenance ;
  sh:property [
                a sh:PropertyShape ;
                sh:path ex:hasNumber ;
                sh:datatype xsd:string ;
              ] .
