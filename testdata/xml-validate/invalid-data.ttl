@prefix onto: <http://validation-9999/onto#> .
@prefix val-onto: <http://api.knora.org/validation-onto#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# WORKS
<http://validation-9999/data/identical-value> a onto:NormalClass ;
    rdfs:label "Value is Identical" ;
    onto:hasSimpleText [ a val-onto:SimpleText ;
            val-onto:hasSimpleTextValue "These two values are identical." ],
        [ a val-onto:SimpleText ;
            val-onto:hasSimpleTextValue "These two values are identical." ] .

<http://validation-9999/data/is-good-value> a onto:NormalClass ;
    rdfs:label "Value is Identical" ;
    onto:hasSimpleText [ a val-onto:SimpleText ;
            val-onto:hasSimpleTextValue "These two values are identical." ] .

# WORKS
<http://validation-9999/data/link-value-wrong> a onto:NormalClass ;
    rdfs:label "Target Link Class Mismatch" ;
    onto:linkProp [ a val-onto:LinkValue ;
            val-onto:hasLinkValueTarget <http://validation-9999/data/link-value-target-wrong> ] .

<http://validation-9999/data/link-value-target-wrong> a onto:NormalClass ;
    rdfs:label "Link Value Target" .


# WORKS
<http://validation-9999/data/max-card-violation> a onto:CardOneResource ;
    rdfs:label "Max Cardinality Violation" ;
    onto:hasSimpleText [ a val-onto:SimpleText ;
            val-onto:hasSimpleTextValue "Text Value Two" ],
        [ a val-onto:SimpleText ;
            val-onto:hasSimpleTextValue "Text Value One" ] .

# WORKS
<http://validation-9999/data/non-existing-list-node> a onto:NormalClass ;
    rdfs:label "List-node does not exist" ;
    onto:listProp [ a val-onto:ListValue ;
            val-onto:hasListName "onlyList" ;
            val-onto:hasListValue "nonExisting" ] .

# WORKS
<http://validation-9999/data/property-type-mismatch> a onto:NormalClass ;
    rdfs:label "Property Type Mismatch" ;
    onto:hasSimpleText [ a val-onto:IntValue ;
            val-onto:hasIntValue "1" ] .

