@prefix ns1: <http://0.0.0.0:3333/ontology/9999/onto/v2#> .
@prefix ns2: <http://api.knora.org/validation-onto#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://validation-9999/data/identical-value> a ns1:NormalClass ;
    rdfs:label "Value is Identical" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "These two values are identical." ],
        [ a ns2:SimpleText ;
            ns2:hasValue "These two values are identical." ] .

<http://validation-9999/data/link-value-wrong> a ns1:NormalClass ;
    rdfs:label "Target Link Class Mismatch" ;
    ns1:linkProp [ a ns2:LinkValue ;
            ns2:hasValue <http://validation-9999/data/link-value-target-wrong> ] .

<http://validation-9999/data/max-card-violation> a ns1:CardOneResource ;
    rdfs:label "Max Cardinality Violation" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "Text Value Two" ],
        [ a ns2:SimpleText ;
            ns2:hasValue "Text Value One" ] .

<http://validation-9999/data/non-existing-list-node> a ns1:NormalClass ;
    rdfs:label "List-node does not exist" ;
    ns1:listProp [ a ns2:ListValue ;
            ns2:hasListName "onlyList" ;
            ns2:hasValue "nonExisting" ] .

<http://validation-9999/data/property-type-mismatch> a ns1:NormalClass ;
    rdfs:label "Property Type Mismatch" ;
    ns1:hasSimpleText [ a ns2:IntValue ;
            ns2:hasValue "1" ] .

<http://validation-9999/data/link-value-target-wrong> a ns1:NormalClass ;
    rdfs:label "Link Value Target" .

