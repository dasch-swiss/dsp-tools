@prefix ns1: <http://0.0.0.0:3333/ontology/9999/onto/v2#> .
@prefix ns2: <http://api.knora.org/validation-onto#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://validation-9999/data/different-prop-identical-value> a ns1:NormalClass ;
    rdfs:label "Duplicate content with different properties is allowed" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "n1" ] ;
    ns1:listProp [ a ns2:ListValue ;
            ns2:hasListName "onlyList" ;
            ns2:hasValue "n1" ] .

<http://validation-9999/data/duplicate-list-different-node> a ns1:NormalClass ;
    rdfs:label "Duplicate list with different nodes are allowed" ;
    ns1:listProp [ a ns2:ListValue ;
            ns2:hasListName "onlyList" ;
            ns2:hasValue "n1" ],
        [ a ns2:ListValue ;
            ns2:hasListName "onlyList" ;
            ns2:hasValue "n1.1" ] .

<http://validation-9999/data/identical-value-1> a ns1:NormalClass ;
    rdfs:label "Value is Identical" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "These two values are identical." ] .

<http://validation-9999/data/identical-value-2> a ns1:NormalClass ;
    rdfs:label "Value is Identical to the others" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "These two values are identical." ] .

<http://validation-9999/data/link-value-correct> a ns1:NormalClass ;
    rdfs:label "Target Link Class Correct" ;
    ns1:linkProp [ a ns2:LinkValue ;
            ns2:hasValue <http://validation-9999/data/inherited-cardinality-correct> ] .

<http://validation-9999/data/inherited-cardinality-correct> a ns1:SubClass ;
    rdfs:label "Inherited good cardinality" ;
    ns1:hasSimpleText [ a ns2:SimpleText ;
            ns2:hasValue "Text Value One" ] .

