@prefix onto:     <http://validation-9999/onto#> .
@prefix val-onto: <http://api.knora.org/validation-onto#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .

<http://validation-9999/data/identical-value-1>
  a                  onto:NormalClass ;
  rdfs:label         "Value is Identical" ;
  onto:hasSimpleText [
                       a                           val-onto:SimpleText ;
                       val-onto:hasSimpleTextValue "These two values are identical."
                     ] .

<http://validation-9999/data/identical-value-2>
  a                  onto:NormalClass ;
  rdfs:label         "Value is Identical to the others" ;
  onto:hasSimpleText [
                       a                           val-onto:SimpleText ;
                       val-onto:hasSimpleTextValue "These two values are identical."
                     ] .

<http://validation-9999/data/link-value-correct>
  a             onto:NormalClass ;
  rdfs:label    "Target Link Class Correct" ;
  onto:linkProp [
                  a                           val-onto:LinkValue ;
                  val-onto:hasLinkValueTarget <http://validation-9999/data/cardinality-correct>
                ] ,
                [
                  a                           val-onto:LinkValue ;
                  val-onto:hasLinkValueTarget <http://validation-9999/data/inherited-cardinality-correct>
                ] .

<http://validation-9999/data/cardinality-correct>
  a                  onto:CardOneResource ;
  rdfs:label         "Inherited good cardinality" ;
  onto:hasSimpleText [
                       a                           val-onto:SimpleText ;
                       val-onto:hasSimpleTextValue "Text Value One"
                     ] .

<http://validation-9999/data/inherited-cardinality-correct>
  a                  onto:SubClass ;
  rdfs:label         "Inherited good cardinality" ;
  onto:hasSimpleText [
                       a                           val-onto:SimpleText ;
                       val-onto:hasSimpleTextValue "Text Value One"
                     ] .

